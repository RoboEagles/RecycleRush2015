    public void eject() {
    	centeringLeft.set(-1.0);
    	centeringRight.set(-1.0);
    }
    
    public void inject() {
    	centeringLeft.set(1.0);
    	centeringRight.set(1.0);
    }
    
    public void spin(int direction) { //DIRECTION 0: LEFT; DIRECTION 1: RIGHT
    	centeringLeft.set(-1.0+(2.0*direction));
    	centeringRight.set(1.0-(2.0*direction));
    }
    
    public void stop() {
    	centeringLeft.set(0.0);
    	centeringRight.set(0.0);
    }
    

